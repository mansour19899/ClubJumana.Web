<!DOCTYPE html>
<html lang="en">

<head>
    <partial name="_MetaTags"/>
    <title>Club Jummana Corp Managment</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="/images/VanmeIcon.ico">
<partial name="_Css"/>

</head>

<body>

<div id="preloader">
    <div class="loader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
        </svg>
    </div>
</div>


<div id="main-wrapper">


<partial name="_Header" />

<partial name="_Sidebar" />

<!--**********************************
Content body start
***********************************-->
<div class="content-body">

    <div class="container-fluid mt-3">        
        <div id="preloaderSub" style="position: fixed;width: 100%;height: 100%;z-index: 1000;background-color: rgb(243, 243, 249);display: none">
            <div class="loader">
                <svg class="circular" viewBox="25 25 50 50">
                    <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
                </svg>
            </div>
        </div>

        @RenderBody()
    </div>
<!-- #/ container -->
</div>
<!--**********************************
Content body end
***********************************-->
<partial name="_Footer" />

</div>

<partial name="_Scripts" />
</body>

</html>
